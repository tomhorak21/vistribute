}</pre></div><div class="paragraph code-paragraph collapsed" data-id="L7buJqfp" name="Study Overwrites" run-on-load="true" last-execution-state="success"><pre data-type="content" type="text/javascript-viscontroller" id="vistributeStudyOverwrites" class="viscontroller">vc = {
  data: 'vistributeStudyOverwrites-data',
  init: function() {    
    this.updateOverwrites = () =&gt; {
      
      if (this.data.studyMode &amp;&amp; !document.body.classList.contains("study")) {
        document.body.classList.add("study");
      } else if (!this.data.studyMode &amp;&amp; document.body.classList.contains("study")) {
        document.body.classList.remove("study");
      }
    };
    
    window.addEventListener("codestrateloaded", () =&gt; {
      if (this.data &amp;&amp; this.data.studyMode)
        this.updateOverwrites();
    });
  },
  update: function(source) {
    if (this.data &amp;&amp; document.querySelector("#vistributeQuickMenu")) {
      this.updateOverwrites();
    }
  }
};</pre></div><div class="paragraph data-paragraph collapsed" data-id="LXPMXECw" name="Study Overwrites Data"><pre data-type="content" type="application/json" id="vistributeStudyOverwrites-data" class="visdata">{
  "config": {},
