}</pre></div><div class="paragraph code-paragraph collapsed" data-id="u6KdrXLs" name="Distribution Logger" run-on-load="true" last-execution-state="success"><pre data-type="content" type="text/javascript-viscontroller" id="vistributeDistributionLogger" class="viscontroller">vc = {
  data: 'vistributeDistributionLogger-data',
  src: ['data', 'devices'],
  init: function() {
    this.log = () =&gt; {
      this.update("vistributeDeviceObserver");
    };
    
    this.onCodestrateLoaded = () =&gt; {
      if (!this.addedListener) {
        window.onerror = (msg, url, line, col, err) =&gt; {
          if (!this.config.active)
            return;

          let data = this.data;
          let errObj = {
            msg: msg,
            url: url,
            line: line,
            col: col,
            client: webstrate.clientId,
            timestamp: Date.now()
          };
          setTimeout(() =&gt; {
            data.errors.push(errObj);
            this.data = data;
          }, Math.random() * 1000);
          
        };
        this.addedListener = true;
      }
    };
    
    window.addEventListener("codestrateloaded", this.onCodestrateLoaded);
    
  },
  update: function(source) {
    if (this.config.active &amp;&amp; source === "vistributeDeviceObserver") {
      if (this.src.data &amp;&amp; this.src.data.output) {
        let cId = this.src.data.data.clientOnDuty;
        if (cId !== webstrate.clientId)
          return;
        
        let data = this.data;
        if (!this.lastSurfaces || (this.lastSurfaces &amp;&amp; Object.keys(this.lastSurfaces).length !== Object.keys(this.src.data.output.surfaces).length)) {
          
          let surs = {
            timestamp: Date.now(),
            surfaces: this.src.data.output.surfaces
          };
          data.surfaces.push(surs);
          this.lastSurfaces = this.src.data.output.surfaces;
        }
        let distr = {
          timestamp: Date.now(),
          distr: this.src.data.output.distribution
        };
        data.distributions.push(distr);
        this.data = data;
      }
    }
  }
};</pre></div><div class="paragraph data-paragraph collapsed" data-id="Hynh43Td" name="Distribution Logger Data"><pre data-type="content" type="application/json" id="vistributeDistributionLogger-data" class="visdata">{
