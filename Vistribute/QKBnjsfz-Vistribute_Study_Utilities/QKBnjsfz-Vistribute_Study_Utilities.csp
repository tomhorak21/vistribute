}</pre></div><div class="paragraph code-paragraph collapsed" data-id="xsfb2SYW" name="Distribution Storage" run-on-load="true" last-execution-state="success"><pre data-type="content" type="text/javascript-viscontroller" id="vistributeDistributionStorage" class="viscontroller">vc = {
  data: 'vistributeDistributionStorage-data',
  src: ['devices'],
  init: function() {
    this.deviceMap = {};
    this.getDistribution = (key) =&gt; {
      let distr = this.data[key];
      let updatedDistr = {};
      let ignoredClients = [];
      
      for (let placeholder in this.deviceMap) {
        if (distr &amp;&amp; distr[placeholder]) {
          updatedDistr[this.deviceMap[placeholder].sId] = distr[placeholder];
        } else {
          ignoredClients = ignoredClients.concat(this.deviceMap[placeholder].cIds);
        }
      }
      console.log(this.deviceMap, ignoredClients, updatedDistr);
      this.output = {
        distr: updatedDistr,
        ignoredClients: ignoredClients
      };
    };
    
    this.updateDeviceMap = () =&gt; {      
      let devices = this.src.devices.output;
      let deviceConfig = this.config.devices;
      for (let placeholder in deviceConfig) {
        if (devices[deviceConfig[placeholder].dId]) {
          let d = devices[deviceConfig[placeholder].dId];
          if (d.clients.length === 1) {
            this.deviceMap[placeholder] = {};
            let cId = d.clients[0];
            this.deviceMap[placeholder].sId = 's-' + d.sessionIds[cId];
            this.deviceMap[placeholder].cIds = [cId];
          } else {
            for (let cId of d.clients) {
              let cRes = d.client_resolutions[cId];
              let res = deviceConfig[placeholder].res;
              if (cRes.width === res.width &amp;&amp; cRes.height === res.height) {
                this.deviceMap[placeholder] = {};
                this.deviceMap[placeholder].sId = 's-' + d.sessionIds[cId];
                if (!this.deviceMap[placeholder].cIds)
                  this.deviceMap[placeholder].cIds = [];
                this.deviceMap[placeholder].cIds.push(cId);
              }
            }
          }
        }
      }
    };
  },
  update: function(source) {
    if (this.src.devices &amp;&amp; this.src.devices.output)
      this.updateDeviceMap();
  }
};</pre></div><div class="paragraph data-paragraph collapsed" data-id="bByJ7s45" name="Distribution Storage Data"><pre data-type="content" type="application/json" id="vistributeDistributionStorage-data" class="visdata">{
