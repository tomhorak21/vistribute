    "studyMode": false
  }
}</pre></div><div class="paragraph style-paragraph collapsed" data-id="6njCdCBf" name="Study Overwrites Styles"><style data-type="content" type="text/css" codemirror="true" id="vistributeStudyOverwrites-styles">body.study #vistributeControlPanel .vistribute.box {
  display: none;
}

body.study #vistributeControlPanel .indicators .spaceReq,
body.study #vistributeControlPanel .indicators .group,
body.study #vistributeControlPanel .view-indicators .spaceReq,
body.study #vistributeControlPanel .view-indicators .group,
body.study #vistributeQuickMenu .view-indicators .spaceReq,
body.study #vistributeQuickMenu .view-indicators .group,
body.study #vistributeQuickMenu #vistributeMinimap {
  display: none !important;
}</style></div><div class="paragraph code-paragraph collapsed" data-id="8Gjg2rVx" name="Study Manager" run-on-load="true" last-execution-state="success"><pre data-type="content" type="text/javascript" id="vistributeStudyManager">var vistribute = vistribute || {};

window.addEventListener("codestrateloaded", () =&gt; {
  (function (defNs) {
    defNs.logDistr = Vistrate.getOutput('vistributeDistributionLogger').log;
    defNs.loadDistr = (distr) =&gt; {
      Vistrate.getOutput('vistributeDistributionLogger').log();
      Vistrate.getOutput('vistributeDistributionStorage').getDistribution(distr);
    };
    defNs.loadDoc = Vistrate.getOutput('signaling').switchDocument;
  })(vistribute);

  (function (window, ns) {
    window.vistribute = ns;
  })(window, vistribute);
});</pre></div></div>